import java.util.Scanner;

public class input {
	// this class will take input from user and then print what they input.
	
	
	public static void main(String[] args) {
		int x=0;
		Scanner keyboard = new Scanner(System.in);
		
		System.out.println("Hello type in a whole number\n");
		x=keyboard.nextInt();
		System.out.println("\n the number you typed was ");
		System.out.println(x);
		System.out.println("\n\n Goodbye");
	}

}
